<head>
  <meta charset="utf-8">
  <%
  var title = page.title;

  if (is_archive()){
    title = __('archive_a');

    if (is_month()){
      title += ': ' + page.year + '/' + page.month;
    } else if (is_year()){
      title += ': ' + page.year;
    }
  } else if (is_category()){
    title = __('category') + ': ' + page.category;
  } else if (is_tag()){
    title = __('tag') + ': ' + page.tag;
  }
  %>
  <title><% if (title){ %><%= title %> | <% } %><%= config.title %></title>
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <% if (theme.favicon){ %>
    <%- favicon_tag(theme.favicon) %>
  <% } %>
  <%- css('css/style') %>
  <link href="/fontawesome/css/all.min.css" rel="stylesheet">
  <%- css('css/prism.css') %>
  <%- css('css/wind-comment.css') %>
  <%- partial('darkreader') %>
  <link rel="stylesheet" href="https://npm.elemecdn.com/katex@0.15.3/dist/katex.min.css">
</head>
